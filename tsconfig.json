  "extends": "@tsconfig/strictest/tsconfig.json",
  "compilerOptions": {
    "strictNullChecks": true,
    "module": "ESNext",
    "target": "ESNext",
    "moduleResolution": "bundler",
    "forceConsistentCasingInFileNames": true,
    "jsx": "react-jsxdev",
    "strict": true,
    "checkJs": true
  }
}